<h1>Edit Your information Here</h1>

<%= form_for @student do |form| %>

<p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
 
  <p>
    <%= form.label :age %><br>
    <%= form.text_field :age %>
  </p>


  <p>
    <%= form.label :grade %><br>
    <%= form.text_field :grade %>
  </p>

  <p>
    <%= form.label :location %><br>
    <%= form.text_field :location %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>

